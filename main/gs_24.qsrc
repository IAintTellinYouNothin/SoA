# gs_24

энергвосст = (1000-$gg['energy'])

if $clock['status']='on':
if ($clock['hours']*60) < (hour*60):сон = (24*60 -(hour*60)) + ($clock['hours']*60)

if сон < энергвосст/100*60:
minutes +=сон
minutes -= minutes mod 60
$gg['energy'] +=сон/60*100+сон/60*60
else
minutes += энергвосст/100*60
minutes -= minutes mod 60

$gg['energy'] += энергвосст + (энергвосст/100)*60
end
end

